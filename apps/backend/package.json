{
  "name": "@enb/backend",
  "version": "0.0.1",
  "scripts": {
    "dev": "NODE_ENV=development nodemon",
    "migrator": "node -r ts-node/register database/migrator.ts",
    "migrator:create": "yarn migrator create --name",
    "migrator:apply": "yarn migrator up",
    "migrator:revert": "yarn migrator down",
    "migrator:list-pending": "yarn migrator pending",
    "migrator:list-executed": "yarn migrator executed",
    "seeder": "node -r ts-node/register database/seeder.ts",
    "seeder:create": "yarn seeder create --name",
    "seeder:apply": "yarn seeder up",
    "seeder:revert": "yarn seeder down",
    "seeder:list-pending": "yarn seeder pending",
    "seeder:list-executed": "yarn seeder executed",
    "test": "NODE_ENV=test jest",
    "test:migrate-db": "NODE_ENV=test yarn migrator up"
  },
  "dependencies": {
    "@fastify/cors": "^8.3.0",
    "dotenv": "^16.3.1",
    "fastify": "^4.22.2",
    "fastify-plugin": "^4.5.1",
    "nodemon": "^3.0.1",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.32.1",
    "umzug": "^3.3.1",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@enb/prettier-config": "workspace:*",
    "@types/jest": "^29.5.5",
    "@types/node": "^18.17.1",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "chalk": "^4.1.2",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^29.6.4",
    "pino-pretty": "^10.2.0",
    "prettier": "3.0.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  }
}
